{
  "name": "zuplo-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run ci:pre && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:scripts": "node ./scripts/build.mjs",
    "format": "prettier --write .",
    "local": "npm run ci:pre",
    "typecheck": "tsc",
    "postinstall": "npm run get-policies && npm run ci:pre",
    "get-policies": "sh ./scripts/get-policies.sh",
    "ci:pre": "npm run ci:bundles && npm run ci:migrate",
    "ci:migrate": "node ./migrate.mjs",
    "ci:bundles": "node ./scripts/update-bundles.mjs"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@algolia/autocomplete-core": "^1.9.2",
    "@headlessui/react": "^1.7.13",
    "@inkeep/widgets": "^0.2.252",
    "@markdoc/markdoc": "^0.3.4",
    "@markdoc/next.js": "^0.3.4",
    "@sindresorhus/slugify": "^2.1.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/json-schema": "^7.0.15",
    "@types/node": "20.4.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-highlight-words": "^0.16.4",
    "autoprefixer": "^10.4.12",
    "clsx": "^1.2.1",
    "fast-glob": "^3.2.12",
    "flexsearch": "^0.7.31",
    "glob": "^10.3.10",
    "js-yaml": "^4.1.0",
    "jsonc-parser": "^3.2.0",
    "lucide-react": "^0.308.0",
    "next": "13.4.16",
    "next-themes": "^0.2.1",
    "prism-react-renderer": "^2.0.6",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-highlight-words": "^0.20.0",
    "shiki": "^0.14.7",
    "simple-functional-loader": "^1.2.1",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.1.0",
    "@types/unist": "^3.0.2",
    "esbuild": "^0.19.11",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.16",
    "gray-matter": "^4.0.3",
    "mdast": "^3.0.0",
    "mdast-util-to-string": "^4.0.0",
