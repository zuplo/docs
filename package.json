{
  "name": "zuplo-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "node ./scripts/check-prepared.js && docusaurus start --port 3002",
    "build": "npm run ci:pre && docusaurus build && npm run ci:post",
    "local": "npm run ci:pre",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "ci:pre": "npm run ci:bundles && npm run ci:policies && npm run ci:reference",
    "ci:post": "npm run ci:redirects",
    "ci:redirects": "node ./scripts/copy-redirects.js",
    "ci:bundles": "node ./scripts/update-bundles.js",
    "ci:reference": "rm -rf ./reference && rm -rf reference.zip && node ./node_modules/.bin/get-build-artifact --owner zuplo --repo core --name runtime-docs --path ./reference.zip && unzip ./reference.zip -d ./reference && rm -rf ./reference.zip && node ./scripts/make-api-reference.js",
    "ci:policies": "npm i @zuplo/policies@latest",
    "update-policies": "node ./scripts/update-policies.js"
  },
  "dependencies": {
    "@brandicons/react": "^1.0.17",
    "@docusaurus/core": "2.0.0-beta.18",
    "@docusaurus/plugin-sitemap": "^2.0.0-beta.18",
    "@docusaurus/preset-classic": "2.0.0-beta.18",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/react": "^1.6.22",
    "@zuplo/policies": "^3.288.0",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.18",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/jsdom": "^16.2.14",
    "@zuplo/dev": "^1.37.0",
    "jsdom": "^19.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
