{
  "name": "zuplo-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "build:scripts": "node ./scripts/build.mjs",
    "format": "prettier --write .",
    "typecheck": "tsc",
    "postinstall": "npm run get-policies && npm run ci:shiki",
    "get-policies": "sh ./scripts/get-policies.sh",
    "ci:shiki": "node ./scripts/copy-shiki.mjs",
    "ci:policies": "node ./scripts/build.mjs && node --enable-source-maps ./scripts/update-policies.main.mjs"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@inkeep/widgets": "^0.2.252",
    "@tailwindcss/typography": "^0.5.10",
    "@types/json-schema": "^7.0.15",
    "@types/mdast": "^4.0.3",
    "@types/node": "^18.19.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/unist": "^2.0.10",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.16",
    "clsx": "^1.2.1",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "hastscript": "^7.2.0",
    "lucide-react": "^0.311.0",
    "mdast-util-to-string": "^3.2.0",
    "next": "^14.0.3",
    "next-mdx-remote": "^4.4.1",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.2.1",
    "posthog-js": "^1.100.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-rewrite": "^3.0.6",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-rehype": "^10.1.0",
    "sharp": "^0.33.2",
    "shiki": "^0.14.7",
    "simple-functional-loader": "^1.2.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "unist-util-visit": "^4.1.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.1.0",
    "esbuild": "^0.19.11",
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "rehype-pretty-code": "^0.9.0",
    "rehype-stringify": "^9.0.4"
  },
  "overrides": {
    "@types/mdast": "^4.0.3",
    "@types/unist": "^2.0.10"
  }
}
