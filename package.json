    "prettier-plugin-tailwindcss": "^0.5.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.12.3",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-rehype": "^11.0.0",
    "sharp": "^0.32.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
  }
