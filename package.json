{
  "name": "z-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build && npm run embed",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "embed:styles": "cd quickstarts && tailwindcss -i ./index.css -o ./dist/index.css",
    "embed:content": "ts-node ./quickstarts/index.tsx",
    "embed:copy": "rm -rf ./build/embed && mkdir ./build/embed && mv ./quickstarts/dist/* ./build/embed",
    "embed": "npm run embed:content && npm run embed:styles"
  },
  "dependencies": {
    "@brandicons/react": "^1.0.17",
    "@docusaurus/core": "2.0.0-beta.18",
    "@docusaurus/preset-classic": "2.0.0-beta.18",
    "@heroicons/react": "^1.0.6",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.18",
    "@tailwindcss/typography": "^0.5.2",
    "@tsconfig/docusaurus": "^1.0.5",
    "classnames": "^2.3.1",
    "jsdom": "^19.0.0",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
