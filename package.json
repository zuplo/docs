{
  "name": "zuplo-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run ci:pre && npm run start:docusaurus",
    "start:policies": "node ./scripts/build.mjs && nodemon --watch policies -e md,json,ts,svg ./scripts/update-policies.mjs",
    "start:docusaurus": "node ./scripts/check-prepared.mjs && docusaurus start --port 3002",
    "build": "npm run ci:pre && docusaurus build",
    "build:scripts": "node ./scripts/build.mjs",
    "format": "prettier --write .",
    "local": "npm run ci:pre",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "ci:pre": "npm run ci:bundles && npm run ci:policies",
    "ci:bundles": "node ./scripts/update-bundles.mjs",
    "ci:policies": "node ./scripts/build.mjs && node ./scripts/update-policies.mjs"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/preset-classic": "^3.0.0",
    "@docusaurus/theme-mermaid": "^3.0.0",
    "@heroicons/react": "^2.0.12",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^1.2.1",
    "json-loader": "^0.5.7",
    "posthog-docusaurus": "^2.0.0",
    "prism-react-renderer": "^2.2.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.10.2",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "shiki": "^0.14.5",
    "unified": "^10.1.2",
    "video.js": "^8.3.0"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "@docusaurus/module-type-aliases": "^3.0.0",
    "@docusaurus/tsconfig": "3.0.0",
    "@docusaurus/types": "3.0.0",
    "@types/glob": "^8.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.15",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.0",
    "esbuild": "^0.19.5",
    "glob": "^8.1.0",
    "jsdom": "^22.0.0",
    "json-schema": "^0.4.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.1.1",
    "ts-node": "^10.8.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
