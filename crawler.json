{
  "index_name": "ZUPLO_SITE",
  "start_urls": [
    {
      "url": "https://zuplo.com/docs",
      "selectors_key": "docs",
      "page_rank": 1
    },
    {
      "url": "https://zuplo.com/blog/",
      "selectors_key": "blog",
      "page_rank": 5
    }
  ],
  "sitemap_urls": ["https://zuplo.com/sitemap.xml"],
  "sitemap_alternate_links": true,
  "stop_urls": [],
  "js_render": true,
  "selectors": {
    "docs": {
      "lvl0": {
        "selector": "(//ul[contains(@class,'menu__list')]//a[contains(@class, 'menu__link menu__link--sublist menu__link--active')]/text() | //nav[contains(@class, 'navbar')]//a[contains(@class, 'navbar__link--active')]/text())[last()]",
        "type": "xpath",
        "global": true,
        "default_value": "Documentation"
      },
      "lvl1": "header h1",
      "lvl2": "article h2",
      "lvl3": "article h3",
      "lvl4": "article h4",
      "lvl5": "article h5, article td:first-child",
      "lvl6": "article h6",
      "text": "article p, article li, article pre, article code, article td:last-child"
    },
    "blog": {
      "lvl0": "article h1",
      "lvl1": "article h2",
      "lvl2": "article h3",
      "lvl3": "article h4",
      "lvl4": "article h5",
      "lvl5": "article h6",
      "text": "article p, article li, article pre, article code, article td:last-child"
    }
  }
}
