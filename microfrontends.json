    "www": {
      "packageName": "website",
      "development": {
        "fallback": "zuplo.com"
      }
    },
    "docs": {
      "routing": [
        {
          "group": "docs",
          "paths": ["/docs", "/docs/:path*"]
        }
      ]
    }
  }
}
