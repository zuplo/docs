{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://zuplo.com/schemas/policies/auth0-jwt-auth-inbound.json",
  "type": "object",
  "title": "Sleep / Delay",
  "isDeprecated": false,
  "isPaidAddOn": false,
  "isUnlisted": false,
  "description": "Add a delay to the incoming request. Useful for testing.\n\n",
  "required": ["handler"],
  "properties": {
    "handler": {
      "type": "object",
      "default": {},
      "required": ["export", "module", "options"],
      "properties": {
        "export": {
          "const": "SleepInboundPolicy",
          "description": "The name of the exported type"
        },
        "module": {
          "const": "$import(@zuplo/runtime)",
          "description": "The module containing the policy"
        },
        "options": {
          "title": "SleepInboundPolicyOptions",
          "type": "object",
          "description": "The options for this policy",
          "additionalProperties": false,
          "required": ["sleepInMs"],
          "properties": {
            "sleepInMs": {
              "type": "number",
              "examples": [1000],
              "description": "The number of milliseconds to delay the request"
            }
          }
        }
      },
      "examples": [
        {
          "_name": "basic",
          "export": "SleepInboundPolicy",
          "module": "$import(@zuplo/runtime)",
          "options": {
            "sleepInMs": 1000
          }
        }
      ]
    }
  }
}
