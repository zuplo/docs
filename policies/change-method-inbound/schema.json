{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://zuplo.com/schemas/policies/auth0-jwt-auth-inbound.json",
  "type": "object",
  "title": "Change Method",
  "isDeprecated": false,
  "isPaidAddOn": false,
  "isUnlisted": false,
  "description": "Changes the HTTP method of the incoming request.\n\n",
  "required": ["handler"],
  "properties": {
    "handler": {
      "type": "object",
      "default": {},
      "required": ["export", "module", "options"],
      "properties": {
        "export": {
          "const": "ChangeMethodInboundPolicy",
          "description": "The name of the exported type"
        },
        "module": {
          "const": "$import(@zuplo/runtime)",
          "description": "The module containing the policy"
        },
        "options": {
          "title": "ChangeMethodInboundPolicyOptions",
          "type": "object",
          "description": "The options for this policy",
          "additionalProperties": false,
          "required": ["method"],
          "properties": {
            "method": {
              "type": "string",
              "examples": ["POST"],
              "description": "The HTTP Method to be used, e.g. POST, GET, PUT, PATCH, etc"
            }
          }
        }
      },
      "examples": [
        {
          "_name": "basic",
          "export": "ChangeMethodInboundPolicy",
          "module": "$import(@zuplo/runtime)",
          "options": {
            "method": "POST"
          }
        }
      ]
    }
  }
}
